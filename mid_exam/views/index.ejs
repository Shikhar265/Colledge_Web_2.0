<!DOCTYPE html>
<html lang="en">
<head>
    <title>Airports</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <h1>List of Airports</h1>
    <a href="/airport/new">Add New Airport</a>
    <ul>
        <% airports.forEach(airport => { %>
            <li>
                <a href="/airports/<%= airport._id %>"><%= airport.airportName %> - <%= airport.city %></a>
            </li>
        <% }) %>
    </ul>
</body>
</html>
